            $(this).stop().animate({
                width:'1200px',
                overflow: 'hidden'
            }, 500)
        }, function () {
            $(this).stop().animate({
                width: '200px',
                overflow: 'hidden'
            }, 300)
        })
        //倒计时,时间过了隐藏广告
        $('#atvCount').each(function () {
            var dataTime = $(this).attr('data_time');
            $(this).countdown(dataTime, function (event) {
                if (event.type == 'finish' || event.type == 'stoped') {
                    $('.fw-bg,.sAtv-show').remove()
                    //$('.advertisement_wrap,.popBg').remove()
